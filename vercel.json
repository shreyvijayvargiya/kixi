{
	"functions": {
		"pages/api/**/*.js": {
			"maxDuration": 30
		}
	},
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "no-store, max-age=0"
				}
			]
		},
		{
			"source": "/manifest.json",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/manifest.json",
			"destination": "/manifest.json"
		}
	]
}
